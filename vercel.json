{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run vercel-build",
  "devCommand": "",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm ci --production=false",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api" },
    { "source": "/:path*", "destination": "/index.html" }
  ]
}
